<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.cenp" ios-CFBundleVersion="4.0.1" version="1.0.7" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="PushbotsPlugin">
        <param name="ios-package" value="PushbotsPlugin" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="Zip">
        <param name="ios-package" value="ZipPlugin" />
    </feature>
    <name>CENP Revista</name>
    <description>
        CENP - Revista
    </description>
    <author email="programador@clarearpropaganda.com.br" href="http://clarearpropaganda.com.br">
        Clarear Propaganda
    </author>
    <icon src="icon.png" />
    <splash height="480" src="www/res/screens/ios/screen-iphone-portrait.png" width="320" />
    <splash height="960" src="www/res/screens/ios/screen-iphone-portrait-2x.png" width="640" />
    <splash height="1024" src="www/res/screens/ios/screen-ipad-portrait.png" width="768" />
    <splash height="2048" src="www/res/screens/ios/screen-ipad-portrait-2x.png" width="1536" />
    <splash height="1136" src="www/res/screens/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <splash height="1334" src="www/res/screens/ios/screen-iphone-portrait-667h.png" width="750" />
    <splash height="2208" src="www/res/screens/ios/screen-iphone-portrait-736h.png" width="1242" />
    <icon height="180" platform="ios" src="www/res/icons/ios/icon-60@3x.png" width="180" />
    <icon height="60" platform="ios" src="www/res/icons/ios/icon-60.png" width="60" />
    <icon height="120" platform="ios" src="www/res/icons/ios/icon-60@2x.png" width="120" />
    <icon height="76" platform="ios" src="www/res/icons/ios/icon-76.png" width="76" />
    <icon height="152" platform="ios" src="www/res/icons/ios/icon-76@2x.png" width="152" />
    <icon height="29" platform="ios" src="www/res/icons/ios/icon-small.png" width="29" />
    <icon height="58" platform="ios" src="www/res/icons/ios/icon-small@2x.png" width="58" />
    <icon height="87" platform="ios" src="www/res/icons/ios/icon-small@3x.png" width="87" />
    <icon height="40" platform="ios" src="www/res/icons/ios/icon-40.png" width="40" />
    <icon height="80" platform="ios" src="www/res/icons/ios/icon-40@2x.png" width="80" />
    <icon height="120" platform="ios" src="www/res/icons/ios/icon-40@3x.png" width="120" />
    <icon platform="android" qualifier="ldpi" src="www/res/icons/android/icon-36-ldpi.png" />
    <icon platform="android" qualifier="mdpi" src="www/res/icons/android/icon-48-mdpi.png" />
    <icon platform="android" qualifier="hdpi" src="www/res/icons/android/icon-72-hdpi.png" />
    <icon platform="android" qualifier="xhdpi" src="www/res/icons/android/icon-96-xhdpi.png" />
    <icon platform="android" qualifier="xxhdpi" src="www/res/icons/android/icon-144-xxhdpi.png" />
    <icon platform="android" qualifier="fr-xxhdpi" src="www/res/icons/android/icon-192-xxxhdpi.png" />
    <content src="index.html" />
    <gap:plugin name="pushbots-cordova-plugin" source="npm" spec="^1.4.0" />
    <access browserOnly="true" origin="*" />
    <access origin="*.pushbots.com" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="android-build-tool" value="gradle" />
</widget>
